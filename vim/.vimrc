execute pathogen#infect()

filetype plugin indent on

syntax enable
syntax on
" let g:solarized_termtrans=1
let g:solarized_termcolors=256
if has('gui_running')
    set background=light
else
    set background=dark
endif
colorscheme solarized

set ruler
set number
set hlsearch


set cursorline
set cursorcolumn